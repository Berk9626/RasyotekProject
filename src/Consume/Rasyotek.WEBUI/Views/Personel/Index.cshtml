@model List<Rasyotek.Business.DTOs.ResultPersonelDto>

<h2>Personel Listesi</h2>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Ad</th>
            <th>Soyad</th>
            <th>Cinsiyet</th>
            <th>Zimmet</th>
            <th>Mezuniyet</th>
            <th>İşlemler</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var personel in Model)
        {
            <tr>
                <td>@personel.Ad</td>
                <td>@personel.Soyad</td>
                <td>@personel.Cinsiyet</td>
                <td>@(personel.Zimmet ? "Evet" : "Hayır")</td>
                <td>@personel.Mezuniyet</td>
                <td>
                    <a href="@Url.Action("Edit", new { id = personel.Id })" class="btn btn-warning">Düzenle</a>
                    <a href="@Url.Action("Delete", new { id = personel.Id })" class="btn btn-danger">Sil</a>
                </td>
            </tr>
        }
    </tbody>

</table>
<a href="@Url.Action("Create")" class="btn btn-primary">Yeni Personel Ekle</a>

